<?php

use Illuminate\Foundation\Testing\RefreshDatabase;
use Tests\TestCase;

class ExampleTest extends TestCase
{
    use RefreshDatabase;

    public function test_database_seeding()
    {
        $this->artisan('db:seed');

        $this->assertDatabaseHas('users', [
            'email' => 'test@example.com',
        ]);
    }
}